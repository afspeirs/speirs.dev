<article {{#ifnotin post.data.page.fileSlug post.data.tags}}id="{{post.data.page.fileSlug}}"{{/ifnotin}}>
	{{#if post.data.title}}
		<header>
			<h3>
				{{post.data.title}}
			</h3>
		</header>
	{{/if}}
	{{#if post.templateContent}}
		{{{post.templateContent}}}
	{{/if}}
	{{#if post.data.link}}
		<p>To view the app, visit <a href="https://{{post.data.link}}" target="_blank" rel="noopener noreferrer">{{post.data.link}}</a>.</p>
	{{/if}}
	{{#if post.data.github}}
		{{> github-corners project=post.data.github}}
	{{/if}}
</article>
