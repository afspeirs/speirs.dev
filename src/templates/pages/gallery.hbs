			{{#times images}}
				<a class="img-wrap" href="assets/img/pages/gallery/large/img_{{this}}.jpg"><img src="assets/img/pages/gallery/img_{{this}}.jpg" alt="Photo {{this}}" /></a>
			{{/times}}
		</div>
	{{/content}}
{{/extend}}