{{#extend "layout"}}
	{{#content "body"}}
		{{>section sectionTitle="gallery"}}
		<div id="content">
			<div id="flex">
				{{#times images}}
					<a class="img-wrap" href="assets/img/gallery/large/img_{{this}}.jpg"><img src="assets/img/gallery/img_{{this}}.jpg"></img></a>
				{{/times}}
			</div>
		</div>
	{{/content}}
{{/extend}}