{
  "name": "speirs.dev",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "vite dev",
    "preview": "vite preview",
    "pwa": "npm run build && npm run preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "outdated": "npx npm-check-updates",
    "outdated:minor": "npx npm-check-updates -t minor",
    "update": "npm run outdated -- -u",
    "update:minor": "npm run outdated:minor -- -u"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.25",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-plugin-svelte": "^2.43.0",
    "mdsvex": "^0.12.3",
    "postcss": "^8.4.41",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/work-sans": "^5.0.20",
    "@fontsource/mada": "^5.0.21"
  }
}
