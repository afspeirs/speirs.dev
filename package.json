{
  "name": "speirs.dev",
  "version": "0.8.0",
  "description": "A website to showcase projects created by AFSpeirs",
  "author": "AFSpeirs",
  "keywords": [
    "11ty",
    "eleventy",
    "parcel"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "debug": "cross-env DEBUG=* npm start",
    "prestart": "npm run clean",
    "start": "cross-env NODE_ENV=development run-p develop:*",
    "clean": "rimraf dist",
    "build:eleventy": "npx eleventy",
    "build:scripts": "webpack",
    "develop:eleventy": "npx eleventy --serve --quiet",
    "develop:scripts": "webpack --watch"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "dayjs": "^1.9.4",
    "eleventy-plugin-sass": "^1.1.1",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/afspeirs/speirs.dev/"
  }
}
