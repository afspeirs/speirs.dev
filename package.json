{
  "name": "speirs.dev",
  "version": "0.5.0",
  "description": "A website to showcase projects created by AFSpeirs",
  "author": "AFSpeirs",
  "keywords": ["11ty", "eleventy", "parcel"],
  "scripts": {
    "build": "ELEVENTY_ENV=production run-s prod:*",
    "debug": "DEBUG=* npm start",
    "start": "ELEVENTY_ENV=development npm-run-all --parallel dev:*",
    "dev:eleventy": "npx eleventy --serve --quiet",
    "dev:parcel": "parcel src/styles/main.scss src/styles/dev.scss src/scripts/main.js src/scripts/dev.js",
    "prod:eleventy": "npx eleventy",
    "prod:parcel": "parcel build src/styles/main.scss src/scripts/main.js"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "sass": "^1.26.10"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/afspeirs/speirs.dev/"
  }
}
