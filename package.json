{
  "name": "speirs.dev",
  "version": "0.6.0",
  "description": "A website to showcase projects created by AFSpeirs",
  "author": "AFSpeirs",
  "keywords": [
    "11ty",
    "eleventy",
    "parcel"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production run-s build:*",
    "debug": "DEBUG=* npm start",
    "prestart": "npm run clean",
    "start": "NODE_ENV=development npm-run-all --parallel develop:*",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "build:eleventy": "npx eleventy",
    "build:scripts": "webpack",
    "develop:eleventy": "npx eleventy --serve --quiet",
    "develop:scripts": "webpack --watch"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "dayjs": "^1.8.30",
    "eleventy-plugin-sass": "^1.0.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/afspeirs/speirs.dev/"
  }
}
